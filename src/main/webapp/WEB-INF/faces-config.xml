<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_3_1.xsd"
	version="3.1">
	
 <component>
  <component-type>org.primefaces.component.ModenaMenu</component-type>
  <component-class>cu.cujae.pweb.los_tankes.modena.component.menu.ModenaMenu</component-class>
 </component>
 <converter>
  <converter-id>EntityConverter</converter-id>
  <converter-class>cu.cujae.pweb.los_tankes.util.EntityConverter</converter-class>
 </converter>
 <managed-bean>
  <managed-bean-name>paisBean</managed-bean-name>
  <managed-bean-class>cu.cujae.pweb.los_tankes.jsf.bean.PaisBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>userBean</managed-bean-name>
  <managed-bean-class>cu.cujae.pweb.los_tankes.jsf.bean.UserBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>formaPagoBean</managed-bean-name>
  <managed-bean-class>cu.cujae.pweb.los_tankes.jsf.bean.FormaPagoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>estadoBean</managed-bean-name>
