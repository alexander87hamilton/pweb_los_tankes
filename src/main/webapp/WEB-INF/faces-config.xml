  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>marcaBean</managed-bean-name>
  <managed-bean-class>cu.cujae.pweb.los_tankes.jsf.bean.MarcaBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>choferBean</managed-bean-name>
  <managed-bean-class>cu.cujae.pweb.los_tankes.jsf.bean.ChoferBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>modeloBean</managed-bean-name>
  <managed-bean-class>cu.cujae.pweb.los_tankes.jsf.bean.ModeloBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>autoBean</managed-bean-name>
  <managed-bean-class>cu.cujae.pweb.los_tankes.jsf.bean.AutoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>turistaBean</managed-bean-name>
  <managed-bean-class>cu.cujae.pweb.los_tankes.jsf.bean.TuristaBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>contratoBean</managed-bean-name>
  <managed-bean-class>cu.cujae.pweb.los_tankes.jsf.bean.ContratoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>languageBean</managed-bean-name>
  <managed-bean-class>cu.cujae.pweb.los_tankes.jsf.bean.LanguageBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <render-kit>
  <renderer>
   <component-family>org.primefaces.component</component-family>
   <renderer-type>org.primefaces.component.ModenaMenuRenderer</renderer-type>
   <renderer-class>cu.cujae.pweb.los_tankes.modena.component.menu.ModenaMenuRenderer</renderer-class>
  </renderer>
 </render-kit>
 <application>
  <locale-config>
   <default-locale>es</default-locale>
  </locale-config>
  <resource-bundle>
   <base-name>i18n.messages</base-name>
   <var>msgs</var>
  </resource-bundle>
  <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
 </application>
 <lifecycle>
  <phase-listener>cu.cujae.pweb.los_tankes.security.util.LoginErrorPhaseListener</phase-listener>
 </lifecycle>
</faces-config>
